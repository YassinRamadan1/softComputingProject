{
  "dataset": {
    "path": "src/main/java/neural_network/case_study/data/banknote.csv",
    "hasHeader": false,
    "trainRatio": 0.8,
    "shuffle": true
  },

  "model": {
    "learningRate": 0.005,
    "epochs": 10,
    "batchSize": 16,
    "threshold": 0.5,
    "loss": "cross_entropy",
    "optimizer": "sgd",
    "initializer": "he"
  },

  "layers": [
    {
      "neurons": 16,
      "activation": "relu"
    },
    {
      "neurons": 8,
      "activation": "relu"
    },
    {
      "neurons": 1,
      "activation": "sigmoid"
    }
  ]
}
